@model HW2_website.Models.Farm

<h2>Farm Details</h2>

<div>
    <h4>Id: @Model.Id</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Kind of Animal</dt>
        <dd>@Model.KindOfAnimal</dd>
        <dt>Name</dt>
        <dd>@Model.Name</dd>
        <dt>Weight</dt>
        <dd>@Model.Weight</dd>
    </dl>
</div>

<div>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</div>